{
  "name": "anything-to-markdown",
  "version": "1.0.0",
  "description": "Convert files and web pages into clean Markdown (HTML, PDF, YouTube, Wikipedia, text)",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "node ./scripts/generate-version.js",
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --runInBand",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "cli": "node dist/cli.cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "anything-to-markdown": "dist/cli.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "axios": "1.13.2",
    "cheerio": "1.0.0-rc.12",
    "commander": "14.0.0",
    "mime-types": "2.1.35",
    "pdf-parse": "2.4.5",
    "turndown": "7.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "tsup": "^6.6.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "anything-to-markdown",
    "markdown",
    "converter",
    "html",
    "youtube",
    "wikipedia",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT"
}
